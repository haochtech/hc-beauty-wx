page {
    background: #f7f7f7;
    padding-top: 10rpx;
    box-sizing: border-box;
}

.nav {
    height: 90rpx;
    background: #fff;
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    display: flex;
    border-bottom: #ddd 2rpx solid;
    z-index: 999;
}

.nav>view {
    flex: 1;
    text-align: center;
    line-height: 90rpx;
    color: #999999;
    font-size: 4vw;
    margin: 0 80rpx;
}

.nav>view.curr {
    color: #444444;
    border-bottom: #444444 4rpx solid;
    box-sizing: border-box;
}

.nav_block {
    height: 92rpx;
}

.date {
    display: flex;
    align-items: center;
    height: 90rpx;
    line-height: 90rpx;
    position: fixed;
    top: 92rpx;
    left: 0;
    width: 100%;
    z-index: 999;
    background: #fff;
    border-bottom: #ddd 2rpx solid;
    box-sizing: border-box;
    padding: 0 20rpx;
}

.date>image {
    width: 40rpx;
    height: 40rpx;
    display: block;
}

.date .sp1 {
    flex: 1;
    box-sizing: border-box;
    padding: 0 20rpx;
}

.date .sp2 {
    color: #bf5557;
}

.date_block {
    height: 92rpx;
}

.list {
    background: #fff;
    padding: 0 20rpx;
}

.list .item {
    padding: 20rpx 0;
    display: flex;
    color: #999999;
}

.list .item+.item {
    border-top: #ddd 2rpx dashed;
}

.list .item>image {
    width: 40rpx;
    height: 40rpx;
    margin-right: 20rpx;
}

.list .item>view:nth-child(2) {
    flex: 1;
}

.list .price {
    color: #191919;
}

.list2 {
    background: #fff;
}

.list2 .item {
    display: flex;
    padding: 20rpx;
    align-items: center;
    color: #999;
    border-bottom: #ddd 2rpx dashed;
}

.list2 .item .sp2 {
    flex: 1;
    padding-left: 20rpx;
    display: flex;
    align-items: center;
    justify-content: flex-end;
}

.list2 .item .sp2>image {
    margin: 0 10rpx;
}

.list2 .item .sp3 {
    color: #333;
}

.list2 .list3 {
    background: #f7f7f7;
    padding: 0 20rpx;
}

.list3 .item2 {
    display: flex;
    padding: 20rpx 0;
    align-items: center;
    color: #999;
    border-bottom: #ddd 2rpx dashed;
}

.list3 .item2 .sp1 {
    flex: 1;
    padding: 0 20rpx;
}