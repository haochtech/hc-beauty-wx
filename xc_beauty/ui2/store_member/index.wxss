page {
    background: #f7f7f7;
}

.top {
    height: 400rpx;
    position: relative;
}

.top .avatar {
    width: 150rpx;
    height: 150rpx;
    border-radius: 100%;
    margin: 0 auto;
}

.top .nick {
    color: #fff;
    font-size: 4vw;
    margin-top: 20rpx;
    text-align: center;
}

.top .top_t {
    position: absolute;
    width: 92vw;
    top: 60rpx;
    left: 4vw;
}

.top .top_t .item_t {
    display: flex;
    padding: 20rpx 30rpx;
    background: #fff;
    border-radius: 20rpx 20rpx 0 0;
    margin-top: 20rpx;
}

.top .top_t .item_t .item_c {
    flex: 1;
    text-align: center;
}

.top .top_t .item_t .item_c+.item_c {
    border-left: #ddd 2rpx dashed;
}

.top .top_t .item_t .item_c>view:nth-child(1) {
    font-size: 6vw;
    color: #ffbf00;
    line-height: 8vw;
}

.top .top_t .item_t .item_c>view:nth-child(2) {
    color: #B7B7B7;
    font-size: 3vw;
}

.top .top_t .item_b {
    display: flex;
    color: #fff;
}

.top .top_t .item_b .item_bb {
    display: flex;
    flex: 1;
    align-items: center;
    justify-content: center;
    padding: 20rpx 0;
}

.top .top_t .item_b .item_bb>image {
    width: 40rpx;
    height: 40rpx;
    margin-right: 20rpx;
}

.top_block {
    height: 130rpx;
}

.list {
    margin-top: 6rpx;
}

.list .title {
    text-align: center;
    background: #fff;
    padding: 20rpx 0;
    font-size: 4.5vw;
    line-height: 6.5vw;
}

.list .title .sp2 {
    width: 160rpx;
    height: 4rpx;
    margin: 10rpx auto 0;
}

.list .item {
    display: flex;
    padding: 20rpx;
    background: #fff;
    margin-top: 6rpx;
}

.list .item>image {
    width: 100rpx;
    height: 100rpx;
    border-radius: 100%;
}

.list .item .item_r {
    margin-left: 20rpx;
    width: 590rpx;
}

.list .item .item_r .item_t {
    display: flex;
}

.list .item .item_r .item_t>view:nth-child(1) {
    flex: 1;
    color: #000;
    font-size: 4vw;
}

.list .item .item_r .item_t .item_btn {
    width: 100rpx;
    height: 40rpx;
    text-align: center;
    line-height: 40rpx;
    color: #fff;
    border-radius: 40rpx;
    background: #aaa;
}

.list .item .item_r .item_b {
    display: flex;
    margin-top: 10rpx;
}

.list .item .item_r .item_b>view {
    flex: 1;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    color: #999999;
}

.list .item .item_r .item_check {
    margin-top: 30rpx;
}

.list .item .item_r .item_check .item_check_btn {
    display: flex;
}

.list .item .item_r .item_check .item_check_btn .sp1 {
    width: 100rpx;
    height: 40rpx;
    text-align: center;
    line-height: 40rpx;
    color: #fff;
    border-radius: 40rpx;
    background: #aaa;
    margin-right: 20rpx;
}

.list .item .item_r .item_check .item_check_content {
    border: #EEEEEE 2rpx solid;
    margin-top: 20rpx;
    height: 140rpx;
    box-sizing: border-box;
    padding: 10rpx 20rpx;
}

.menu {
    position: fixed;
    top: 15vh;
    width: 92vw;
    background: #fff;
    border-radius: 10rpx;
    z-index: 1000;
    left: 4%;
    box-sizing: border-box;
    padding: 20rpx 40rpx;
}

.menu .title {
    padding: 20rpx;
    text-align: center;
    font-size: 4.5vw;
}

.menu .menu_content {
    padding: 20rpx 0;
    max-height: 30vh;
    overflow: scroll;
}

.menu .menu_content .item {
    display: flex;
    line-height: 6vw;
}

.menu .menu_content .item>view:nth-child(1) {
    color: #999999;
}

.menu .menu_content .item>view:nth-child(2) {
    flex: 1;
}

.menu .menu_tip {
    display: flex;
    padding: 20rpx 0;
    border-top: #ddd 2rpx solid;
    color: #999999;
}

.menu .menu_tip view+view {
    margin-left: 20rpx;
}

.menu .menu_btn {
    width: 400rpx;
    height: 80rpx;
    color: #fff;
    background: #aaa;
    display: flex;
    align-items: center;
    justify-content: center;
    margin: 20rpx auto;
    border-radius: 10rpx;
    font-size: 4vw;
}

.menu .menu_close {
    position: absolute;
    top: 20rpx;
    right: 20rpx;
}

.menu2 {
    position: fixed;
    top: 15vh;
    width: 94vw;
    background: #fff;
    z-index: 1000;
    left: 3vw;
    box-sizing: border-box;
    padding: 0 30rpx;
}

.menu2 .menuTop {
    padding: 30rpx 0;
    display: flex;
    align-items: center;
}

.menu2 .menuTop>image {
    width: 100rpx;
    height: 100rpx;
    border-radius: 100%;
}

.menu2 .menuTop .menuR {
    flex: 1;
    margin-left: 20rpx;
}

.menu2 .menuTop .menuR .sp1 {
    color: #999999;
}

.menu2 .menuTop .menuR .sp2 {
    color: #ffc926;
}

.menu2 .menuTT {
    font-size: 4.5vw;
    line-height: 6.5vw;
}

.menu2 .itemC {
    padding: 10rpx 0;
}

.menu2 .itemC .sp {
    padding: 10rpx 0;
    display: flex;
    align-items: center;
}

.menu2 .itemC .sp.on {
    color: #999999;
}

.menu2 .itemC .sp>view {
    width: 20%;
    text-align: center;
}

.menu2 .itemC .btn {
    width: 100rpx;
    text-align: center;
    color: #fff;
    height: 50rpx;
    border-radius: 50rpx;
    line-height: 50rpx;
    margin: 0 auto;
}

.menu2 .menuYY {
    padding: 40rpx 0;
}

.menu2 .menuYY .sp3 {
    color: #777777;
}

.menu2 .menu_close {
    position: absolute;
    top: 20rpx;
    right: 20rpx;
}

.menu2 .menu_close>icon {
    display: flex;
    align-items: center;
    justify-content: center;
}

.nav3 {
    padding: 20rpx 0;
    overflow: hidden;
    background: #fff;
}

.nav3 .item {
    text-align: center;
    float: left;
    width: 33.3333333333333333%;
}

.nav3 .item:nth-child(3n+1),.nav3 .item:nth-child(3n+2),.nav3 .item:nth-child(3n+3) {
    margin-top: 20rpx;
}

.nav3 .item>image {
    margin: 0 auto;
    margin-bottom: 10rpx;
}

.yin {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100vh;
    background: rgba(0,0,0,0.5);
    z-index: 200;
}

.yin .yins {
    position: fixed;
    width: 650rpx;
    top: 150rpx;
    left: 50rpx;
    z-index: 201;
}

.yins_list {
    background: #fff;
}

.yins_list .yins_list_t {
    padding: 30rpx 0;
    text-align: center;
    border-bottom: #ddd 2rpx dashed;
}

.yins_list .yins_list_t>image {
    width: 150rpx;
    height: 150rpx;
    border-radius: 100%;
    margin: 0 auto;
}

.yins_list .yins_list_t>view:nth-child(2) {
    margin-top: 10rpx;
}

.yins_list .yins_list_t>view:nth-child(3) {
    color: #999999;
}

.yins_list .yins_code {
    padding: 80rpx 0;
}

.yins_list .yins_code>image {
    width: 350rpx;
    height: 350rpx;
    margin: 0 auto;
}

.yins_btn {
    width: 300rpx;
    height: 80rpx;
    border-radius: 80rpx;
    color: #fff;
    background: #444;
    margin: 0 auto;
    line-height: 80rpx;
    text-align: center;
    margin-top: 50rpx;
}

.yin_close {
    position: absolute;
    display: flex;
    top: -70rpx;
    right: 0;
    align-items: center;
}

.line {
    display: flex;
    padding: 20rpx 30rpx;
    align-items: center;
    background: #fff;
    border-bottom: #eee 2rpx solid;
}

.line .sp1 {
    flex: 1;
}