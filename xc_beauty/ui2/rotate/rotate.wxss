page {
    line-height: 1;
    color: #333;
}

.body {
    position: relative;
}

.RC1 {
    height: 240rpx;
    position: relative;
}

.RC1 .RCback {
    width: 100%;
    height: 100%;
}

.RC1 .announce {
    width: 500rpx;
    height: 90rpx;
    box-sizing: border-box;
    margin: 0 auto;
    border-radius: 90rpx;
    background-color: rgba(0,0,0,0.5);
    padding: 0 20rpx;
    position: absolute;
    top: 40rpx;
    left: 50%;
    transform: translate(-50%);
}

.RC1 .announce .icon {
    width: 33rpx;
    margin-right: 10rpx;
}

.RC1 swiper {
    height: 60rpx;
    line-height: 30rpx;
    overflow: hidden;
    font-size: 26rpx;
    color: #fff;
    margin: 15rpx 0;
}

.RC1 swiper .sp1 {
    color: #ffdc44;
}

.RC1 swiper swiper-item {
    height: 60rpx;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
    overflow: hidden;
}

.RC2 {
    margin: 0 auto;
    background-color: #fff;
    position: relative;
    padding: 112rpx 30rpx 70rpx;
}

.RC2 .userImg {
    text-align: center;
    width: 124rpx;
    height: 124rpx;
    padding: 3rpx;
    background-color: #fff;
    box-shadow: 1px 1px 3px #666,-1px -1px 3px #666;
    border-radius: 50%;
    position: absolute;
    left: 50%;
    margin-left: -65rpx;
    top: -65rpx;
}

.RC2 .userImg image {
    width: 124rpx;
    height: 124rpx;
    border-radius: 50%;
}

.RC2 .signBlocks {
    text-align: center;
}

.RC2 .signBlocks .sBlock {
    display: inline-block;
    width: 80rpx;
    height: 80rpx;
    line-height: 80rpx;
    border-radius: 3px;
    box-shadow: 1px 1px 3px #999,-1px -1px 3px #999;
    margin: 0 20rpx;
    font-size: 48rpx;
    position: relative;
    overflow: hidden;
    margin-bottom: 40rpx;
    color: #999;
}

.RC2 .signBlocks .sBlock image {
    position: absolute;
    width: 80rpx;
    height: 80rpx;
    left: 0;
    top: 0;
}

.RC2 .signBtn {
    width: 210rpx;
    height: 60rpx;
    line-height: 60rpx;
    text-align: center;
    margin: 0 auto;
    margin-bottom: 30rpx;
    font-size: 24rpx;
    color: #fff;
    background-color: #fca9e0;
    border-radius: 30rpx;
}

.RC2 .signBtn2 {
    width: 210rpx;
    height: 60rpx;
    line-height: 60rpx;
    text-align: center;
    margin: 0 auto;
    margin-bottom: 30rpx;
    font-size: 24rpx;
    color: #fff;
    background-color: #d4d3d1;
    border-radius: 30rpx;
}

.RC2 .signText1 {
    margin-bottom: 15rpx;
    text-align: center;
    font-size: 22rpx;
    color: #fca9e0;
}

.RC2 .signText2 {
    font-size: 22rpx;
    line-height: 30rpx;
    text-align: center;
}

.RC2 .signText2 .sp1 {
    color: #fca9e0;
}

.plate-wrap-box {
    position: relative;
    z-index: 98;
    width: 500rpx;
    height: 500rpx;
    border-radius: 50%;
    margin: 30rpx auto 40rpx;
}

.plate-border {
    position: relative;
    z-index: 9;
    width: 500rpx;
    height: 500rpx;
    background: -webkit-linear-gradient(right top,#fc9adc 0%,#9bf0fd 50%,#fc9adc 100%);
    border-radius: 50%;
}

.plate-wrap {
    position: absolute;
    left: 50%;
    top: 50%;
    transform: translate(-50%,-50%);
    z-index: 9;
    width: 440rpx;
    height: 440rpx;
    background-color: #fff;
    border-radius: 50%;
    overflow: hidden;
    margin: auto;
}

.plate-light {
    z-index: 1;
    width: 500rpx;
    height: 500rpx;
    border: none;
    background-color: rgba(0,0,0,0);
}

.plate-box {
    position: absolute;
    z-index: 1;
    left: 50%;
    width: 0;
    height: 0;
    font-size: 24rpx;
}

.bulb {
    position: absolute;
    width: 16rpx;
    height: 16rpx;
    left: 50%;
    top: -242rpx;
    transform: translate(-50%,0);
    border-radius: 50%;
    background-color: #fff;
    filter: blur(0.5rpx);
}

.text-box {
    position: absolute;
    text-align: center;
    display: inline-block;
    width: 100rpx;
    top: -180rpx;
    transform: translate(-50%,0);
    font-size: 28rpx;
    line-height: 1.2;
}

.plate-btn-wrap {
    position: absolute;
    z-index: 10;
    left: 50%;
    top: 50%;
    transform: translate(-50%,-50%);
    width: 110rpx;
    height: 110rpx;
    border-radius: 50%;
}

.plate-btn-wrap::before {
    content: '';
    position: absolute;
    z-index: 9;
    left: calc(50% - 10rpx);
    top: -40rpx;
    transform: translate(-50%,0);
    width: 0;
    height: 0;
    border-bottom: 80rpx solid #fdcae3;
    border-left: 20rpx solid transparent;
}

.plate-btn-wrap::after {
    content: '';
    position: absolute;
    z-index: 9;
    left: calc(50% + 10rpx);
    top: -40rpx;
    transform: translate(-50%,0);
    width: 0;
    height: 0;
    border-bottom: 80rpx solid #fce6e6;
    border-right: 20rpx solid transparent;
}

.plate-btn-box {
    position: relative;
    z-index: 11;
    width: 100%;
    height: 100%;
    border-radius: 50%;
    background: -webkit-linear-gradient(left top,#ffade0 0%,#fce6e6 50%,#ffade0 100%);
    display: flex;
    justify-content: center;
    align-items: center;
}

.plate-inner-btn {
    position: relative;
    z-index: 999;
    width: 90rpx;
    height: 90rpx;
    border-radius: 50%;
    background-color: #fc9adc;
    font-size: 28rpx;
    color: #fff;
    display: flex;
    justify-content: center;
    align-items: center;
}

.RC2 .signNotice {
    font-size: 24rpx;
    color: #666;
}

.RC2 .signNotice .h1 {
    font-size: 26rpx;
    text-align: center;
    margin-bottom: 15rpx;
}

.RC2 .signNotice .border {
    width: 40rpx;
    height: 2px;
    background-color: #fc9adc;
    margin: 10rpx auto;
}

.RC2 .signNotice .h2 {
    font-size: 30rpx;
    text-align: center;
    margin-bottom: 20rpx;
}

.RC2 .signNotice .p {
    line-height: 40rpx;
    padding-left: 40rpx;
    display: block;
}