.nav {
    padding: 20rpx;
    display: flex;
}

.nav>view {
    flex: 1;
    text-align: center;
}

.list {
    margin: 20rpx 30rpx;
    border-radius: 20rpx;
    box-shadow: 0px 0px 10rpx 2rpx #fdeef3;
    padding: 0 20rpx 30rpx;
    color: #999999;
}

.list.theme3 {
    box-shadow: 0px 0px 10rpx 2rpx #ddd;
}

.list>view:nth-child(1) {
    padding: 20rpx 0;
}

.list .item_d {
    display: flex;
    padding: 20rpx 0;
    align-items: center;
}

.list .item_d+.item_d {
    border-top: #ddd 2rpx solid;
}

.list .item_d>view {
    width: 150rpx;
}

.list .item_d>input {
    flex: 1;
    box-sizing: border-box;
    padding: 0 20rpx;
}

.list .item {
    border-top: #ddd 2rpx solid;
    border-bottom: #ddd 2rpx solid;
}

.list .item .item_t {
    padding: 20rpx 0;
}

.list .item .item_b {
    padding: 20rpx 0;
    color: #000000;
    display: flex;
    padding-left: 50rpx;
}

.list .item .item_b>view:nth-child(1) {
    font-size: 4.5vw;
}

.list .item .item_b input {
    padding: 0 20rpx;
    flex: 1;
    font-size: 8vw;
}

.list .tip {
    padding: 20rpx 0;
    font-size: 3vw;
}

.list .btn {
    margin: 30rpx 80rpx 0;
    color: #fff;
    display: flex;
    height: 80rpx;
    align-items: center;
    justify-content: center;
    border-radius: 10rpx;
}

.list2 {
    background: #fff;
    padding: 4vw 2vw;
}

.list2 .title {
    text-align: center;
    font-size: 4.5vw;
}

.list2 .content {
    overflow: hidden;
    margin-top: 4vw;
}

.list2 .content .item {
    float: left;
    width: 42vw;
    margin: 0 2vw 20rpx;
    display: flex;
    padding: 20rpx 0;
    justify-content: center;
    align-items: center;
    border: #ddd 2rpx solid;
    border-radius: 20rpx;
    position: relative;
    flex-direction: column;
}

.list2 .item .tip {
    position: absolute;
    top: 0;
    left: 20rpx;
    width: 40rpx;
}

.list2 .item .tip>image {
    width: 100%;
    display: block;
}

.list2 .item .tip>view {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    color: #fff;
    text-align: center;
    font-size: 2.5vw;
    line-height: 2.5vw;
    box-sizing: border-box;
    padding: 0 10rpx;
}

.list2 .content .item>view:nth-child(1) {
    font-size: 6vw;
    color: #aaa;
}

.list2 .content .item>view:nth-child(2) {
    color: #12b8f6;
}

.list2 .btn {
    margin: 30rpx 80rpx 0;
    color: #fff;
    display: flex;
    height: 80rpx;
    align-items: center;
    justify-content: center;
    border-radius: 10rpx;
}